        "verses": [
          {
            "chords": "  E           B7                    E",
            "lyrics": "Lutar devemos todos, com fé e com valor,"
          },
          {
            "chords": "               B7                    E",
            "lyrics": "Vestidos de justiça, com armas do Senhor;"
          },
          {
            "chords": "   B7          E       F#         B7 ",
            "lyrics": "Cingidos da verdade, podemos pelejar;"
          },
          {
            "chords": "   E            A       B7         E",
            "lyrics": "Calçados no_Evangelho, iremos triunfar."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "E             B7             E",
            "lyrics": "De Deus a_armadura devemos tomar"
          },
          {
            "chords": "A          E       B7          E",
            "lyrics": "Para, na peleja, podermos triunfar."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "   E           B7                  E",
            "lyrics": "Da fé o_escudo temos, podemos batalhar;"
          },
          {
            "chords": "            B7                E",
            "lyrics": "Avante! Valorosos e sem desanimar;"
          },
          {
            "chords": "   B7        E          F#          B7 ",
            "lyrics": "Combateremos firmes com celestial valor,"
          },
          {
            "chords": "  E         A        B7         E",
            "lyrics": "E tudo venceremos em nome do Senhor."
          }
        ]
      },
      {

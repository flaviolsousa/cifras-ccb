  "rhythm": "Canção",
  "score": {
    "introduction": ["D", "A", "G", "D"],
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "D           A      G        D",
            "lyrics": "Cristo, meu Mestre e meu Senhor,"
          },
          {
            "chords": "G      D         E7          A",
            "lyrics": "Eu Te adoro, por fé, com fervor;"
          },
          {
            "chords": "D        A        G      A",
            "lyrics": "Rogo que guardes meu coração;"
          },
          {
            "chords": "G        D         A      D ",
            "lyrics": "Vem protege-lo com Tua unção"
          },
          {
            "chords": "G      D         A7     D",
            "lyrics": "E defendê-lo, ó meu Guardião."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "D      A       G         D",
            "lyrics": "Mestre divino, sempre senti"
          },
          {
            "chords": " G       D       E7       A",
            "lyrics": "Meu coração dependente de Ti;"
          },
          {
            "chords": " D       A     G         A",
            "lyrics": "Bom fundamento dá-lhe, Senhor,"
          },
          {
            "chords": "G         D        A       D",
            "lyrics": "Dá-lhe firmeza, virtude, valor"
          },
          {
            "chords": "G      D       A7       D",
            "lyrics": "E fortaleza, ó meu Protetor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "D       A       G       D",
            "lyrics": "Vale profundo, cheio de mal,"
          },
          {
            "chords": "G       D       E7      A",
            "lyrics": "É este mundo, ó Rei divinal."
          },
          {
            "chords": "D      A     G       A",
            "lyrics": "Só Tua força pode manter"
          },
          {
            "chords": " G      D        A          D",
            "lyrics": "Santo e puro, na graça, meu ser,"
          },
          {
            "chords": " G       D         A7     D",
            "lyrics": "Sempre seguro, com fé e poder."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "4",
        "verses": [
          {
            "chords": " D        A      G       D",
            "lyrics": "Mestre piedoso, com Tua mão,"
          },
          {
            "chords": "G       D       E7      A",
            "lyrics": "Faze perfeito o meu coração,"
          },
          {
            "chords": " D         A    G        A",
            "lyrics": "Santo, ardoroso em Te servir,"
          },
          {
            "chords": " G        D        A          D",
            "lyrics": "Sempre voltado aos bens do porvir;"
          },
          {
            "chords": " G      D       A7        D",
            "lyrics": "Mestre amado, desejo Te ouvir."
          }
        ]
      }
    ]
  }
}

        "verses": [
          {
            "chords": "  B               E          B",
            "lyrics": "Seguro Alicerce é o Senhor Jesus,"
          },
          {
            "chords": "                             C#7          F#7 ",
            "lyrics": "Que nos tirou das trevas, mostrando-nos a luz,"
          },
          {
            "chords": "  B                   E           B",

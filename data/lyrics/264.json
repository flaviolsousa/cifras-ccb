{
  "version": "v2",
  "title": "Redentor celeste e santo",
  "tone": "C",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "C                              G        C",
            "lyrics": "Redentor celeste_e santo, puro é o Teu amor;"
          },
          {
            "chords": "                        F    G       G7      C",
            "lyrics": "Tu me deste a_esperança de entrar no céu, Senhor."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "G7               C",
            "lyrics": "Oh! Quão feliz serei no dia"
          },
          {
            "chords": "F        G      D7     G ",
            "lyrics": "Em que Jesus do céu voltar;"
          },
          {
            "chords": "C                 F      C",
            "lyrics": "Junto aos santos, que_alegria,"
          },
          {
            "chords": "F      G       G7    C",
            "lyrics": "O galardão vou desfrutar!"
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "C                                      G         C",
            "lyrics": "Faz, Senhor, com que_eu me_aparte do pecado e do mal,"
          },
          {
            "chords": "                     F       G     G7   C",
            "lyrics": "E medite noite_e dia sobre_a lei celestial."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "C                             G            C",
            "lyrics": "Bem firmado na_esperança, a vitória_alcançarei;"
          },
          {
            "chords": "                        F      G     G7   C",
            "lyrics": "Prosseguindo confiante, no Teu reino entrarei."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }

  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "     C                          F         C",
            "lyrics": "O Senhor Jesus passava, com Ele ia_a multidão;"
          },
          {
            "chords": "           G                C          G              C",
            "lyrics": "Eis que_um cego, junto_ao caminho, esperava por compaixão;"
          },
          {
            "chords": "          F        C                 G ",
            "lyrics": "E ouvindo que se falava sobre_o nome de Jesus,"
          },
          {
            "chords": "C             F          C               G      C",
            "lyrics": "Levantou-se_e foi ao bom Mestre para receber a luz."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "    C                       F         C ",
            "lyrics": "A Palavra virtuosa, tão esperada, escutou;"
          },
          {
            "chords": "    G                 C           G                C",
            "lyrics": "A visão lhe foi restaurada e, por fé, também se salvou;"
          },
          {
            "chords": "          F         C                      G ",
            "lyrics": "Libertado por Jesus Cristo, dava glória_ao grande Deus;"
          },
          {
            "chords": "C         F        C                G       C",
            "lyrics": "Exaltou o Mestre divino e seguiu os passos Seus."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "      C                            F            C",
            "lyrics": "Jesus Cristo é o mesmo, entre_os fiéis também está,"
          },
          {
            "chords": "        G              C             G                 C",
            "lyrics": "E, quem crer, verá Sua glória; Seu favor, por certo, terá."
          },
          {
            "chords": "        F        C                       G ",
            "lyrics": "Ele é a Luz preciosa que, dos céus, Deus enviou;"
          },
          {
            "chords": "C              F       C                G   C",
            "lyrics": "Quem em trevas se encontrava, essa Luz iluminou."
          }
        ]
      }
    ]
  }
}
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "    C",
            "lyrics": "Não volto ao pecado, sigo meu Senhor;"
          },
          {
            "chords": "    F           C                    G ",
            "lyrics": "Por fé eu já desfruto as bênçãos de amor"
          },
          {
            "chords": "    C",
            "lyrics": "Que foram prometidas pelo Criador;"
          },
          {
            "chords": "   F            C       G           C",
            "lyrics": "No céu, já me esperam riquezas de valor."
          }
        ]

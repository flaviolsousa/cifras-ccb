{
  "version": "v2",
  "title": "Eleitos nós somos",
  "tone": "G",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "  G                    D            G",
            "lyrics": "Eleitos nós somos por Deus, o Verdadeiro,"
          },
          {
            "chords": "                     A7          D    ",
            "lyrics": "E fomos remidos por Seu grande amor;"
          },
          {
            "chords": "  G                    D          G",
            "lyrics": "Vivemos por Cristo, o divinal Cordeiro;"
          },
          {
            "chords": "     C          G       D         G",
            "lyrics": "Oh! Glória! Aleluia! Hosana ao Senhor!"
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "    G",
            "lyrics": "Nós somos eleitos por Deus, Criador;"
          },
          {
            "chords": "    C        G         D         G",
            "lyrics": "Nos céus fruiremos a herança de amor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "    G               D            G",
            "lyrics": "Do trono celeste, poder nós buscaremos,"
          },
          {
            "chords": "                      A7       D    ",
            "lyrics": "E chuvas de bênçãos dará o Criador;"
          },
          {
            "chords": "    G                  D             G",
            "lyrics": "Na graça gloriosa, por fé, nós cresceremos;"
          },
          {
            "chords": "     C          G      D          G",
            "lyrics": "Oh! Glória! Aleluia! Hosana ao Senhor!"
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "  G                    D                G",
            "lyrics": "Está perto a volta de Cristo, o Rei da glória;"
          },
          {
            "chords": "                       A7       D    ",
            "lyrics": "Irmãos, aguardemos com fé e fervor;"
          },
          {
            "chords": "    G                D        G",
            "lyrics": "No céu cantaremos o hino da vitória;"
          },
          {
            "chords": "     C          G      D          G",
            "lyrics": "Oh! Glória! Aleluia! Hosana ao Senhor!"
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
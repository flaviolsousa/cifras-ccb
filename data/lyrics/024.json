      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "     G                  D          G      ",
            "lyrics": "Esse elo de_amor sempre me faz lembrar"
          },
          {
            "chords": "        Am       A          D",
            "lyrics": "Quanto_amaste minh´alma, Senhor;"
          },
          {
            "chords": "      G                   D          G      ",
            "lyrics": "Quero sempre sentir, cultivar esse_amor,"
          },
          {
            "chords": "     C          D           G",

{
  "version": "v2",
  "title": "Dá-me graça, Senhor",
  "tone": "G",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "       G                   D         G      ",
            "lyrics": "Dá-me graça, Senhor, para sempre Te_amar"
          },
          {
            "chords": "      Am          A     D",
            "lyrics": "Com intenso_e profundo amor"
          },
          {
            "chords": "       G                D      G      ",
            "lyrics": "Que procede de Ti, glorioso Senhor,"
          },
          {
            "chords": "      C          D           G",
            "lyrics": "E me faz Teus conselhos guardar."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "       C          D         G     E              ",
            "lyrics": "Quero_amar-Te com todo_o fervor,"
          },
          {
            "chords": "   Am          A7        D                                   ",
            "lyrics": "Adorar-Te, meu bom Salvador;"
          },
          {
            "chords": "          G                C     Am ",
            "lyrics": "Vem, com Tuas virtudes, cingir"
          },
          {
            "chords": "                  D          G",
            "lyrics": "O Teu servo, que quer Te servir."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "      G                   D            G      ",
            "lyrics": "Com divina_afeição quero sempre Te_honrar"
          },
          {
            "chords": "     Am        A        D",
            "lyrics": "E também ser fiel servidor;"
          },
          {
            "chords": "       G                D          G      ",
            "lyrics": "Esse_imenso amor que de Ti vem, Senhor,"
          },
          {
            "chords": "     C           D         G",
            "lyrics": "Me ensina_ao Teu Nome_exaltar."
          }
        ]
      },
      {
        "type": "ref",

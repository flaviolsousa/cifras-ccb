      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "C              F       C            G ",
            "lyrics": "O Teu precioso sangue, ó divinal Cordeiro,"
          },
          {
            "chords": "C             F           C    G7     C",
            "lyrics": "Do mundo resgatou-me, por Teu imenso_amor;"
          },
          {
            "chords": "G                 D  D7           G ",
            "lyrics": "Faz que em Ti esteja a minha confiança,"
          },
          {
            "chords": "C            F       C     G7   C",
            "lyrics": "Dá-me perseverança e fé, ó Redentor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "C                F      C                G ",
            "lyrics": "Fraco eu sou, no mundo, para_enfrentar a lida,"
          },
          {
            "chords": "C                F        C       G7  C",
            "lyrics": "Mas, com celeste força, o mal não temerei;"
          },
          {
            "chords": "G            D  D7                G ",
            "lyrics": "Vindo o inimigo para_enlaçar-me_a vida,"
          },
          {
            "chords": "C                F       C       G7     C",
            "lyrics": "Sei que terei guarida em Ti, meu grande Rei."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "C               F     C             G ",
            "lyrics": "Estando sempre_atento à celestial Palavra,"
          },
          {
            "chords": "C              F       C      G7     C",
            "lyrics": "Nunca serei vencido do mal, ó meu Senhor."
          },
          {
            "chords": "G               D  D7            G ",
            "lyrics": "O Teu falar divino confortará minha´alma,"
          },
          {
            "chords": "C               F         C    G7   C",
            "lyrics": "Dando-lhe paz e calma, firmeza e fervor."
          }
        ]
      }

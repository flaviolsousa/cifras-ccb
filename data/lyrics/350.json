{
  "version": "v2",
  "title": "A jornada farei",
  "tone": "D",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "     D                            G ",
            "lyrics": "A jornada farei com Jesus, meu Senhor,"
          },
          {
            "chords": "     D                 A  A7 ",
            "lyrics": "Certamente ao céu chegarei;"
          },
          {
            "chords": "    D                                    G    ",
            "lyrics": "Peregrino_aqui sou, busco_o lar de_esplendor,"
          },
          {
            "chords": "        D         E         A  A7 ",
            "lyrics": "Onde_os bens celestiais fruirei."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "D                            A ",
            "lyrics": "Sempre_unido_a Cristo devo estar,"
          },
          {
            "chords": "                   A7      D",
            "lyrics": "Para, nos perigos, não vacilar;"
          },
          {
            "chords": "                              G ",
            "lyrics": "Sinto a voz divina em meu coração,"
          },
          {
            "chords": "          D    A       D",
            "lyrics": "Que me oferece o galardão."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "     D                          G ",
            "lyrics": "A jornada será, para mim, um prazer,"
          },
          {
            "chords": "     D                  A  A7 ",
            "lyrics": "Caminhando com meu Salvador;"
          },
          {
            "chords": "         D                         G ",
            "lyrics": "Quero_ao mal renunciar e a graça reter,"
          },
          {
            "chords": "      D            E         A  A7 ",
            "lyrics": "Quero ser, sobre_o mal, vencedor."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "     D                          G ",
            "lyrics": "A jornada farei com a força de Deus;"
          },
          {
            "chords": "      D                 A  A7 ",
            "lyrics": "Nos perigos, guardado serei,"
          },
          {
            "chords": "       D                           G ",
            "lyrics": "Pois comigo está Quem me leva aos céus;"
          },
          {
            "chords": "      D       E          A  A7 ",
            "lyrics": "Bem seguro, então, marcharei."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
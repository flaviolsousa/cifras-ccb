        "verses": [
          {
            "chords": "C                   F ",
            "lyrics": "Na cidade santa do eterno Deus,"
          },
          {
            "chords": "G                     C",
            "lyrics": "Pecador não poderá entrar;"
          },
          {
            "chords": "                       F ",
            "lyrics": "Lá só entrarão os escolhidos Seus"
          },
          {
            "chords": "G                          C",
            "lyrics": "Para_os bens eternos desfrutar."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "C                         F    D7 ",
            "lyrics": "Na cidade santa, por Deus preparada,"
          },
          {
            "chords": "G                     C ",
            "lyrics": "Um repouso_eterno o fiel terá;"
          },
          {
            "chords": "                           F   D7 ",
            "lyrics": "De_eternais riquezas ela_é adornada"
          },
          {
            "chords": "G                        C",
            "lyrics": "E seu brilho não se_apagará."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "C                  F ",
            "lyrics": "A cidade santa do eterno Rei"
          },
          {
            "chords": "G                       C",
            "lyrics": "É de_incomparável esplendor;"
          },
          {
            "chords": "                        F ",
            "lyrics": "Nela_habitará quem caminhar na lei,"
          },
          {
            "chords": "G                       C",
            "lyrics": "Na perfeita lei do Redentor."
          }
        ]
      },
      {

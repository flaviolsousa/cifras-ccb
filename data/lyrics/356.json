{
  "version": "v2",
  "title": "Por preço elevado Jesus resgatou-me",
  "tone": "D",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "    D               A ",
            "lyrics": "Por preço_elevado Jesus resgatou-me,"
          },
          {
            "chords": "                D",
            "lyrics": "Por preço que_o ouro não pode pagar;"
          },
          {
            "chords": "                         G          D",
            "lyrics": "Com Seu próprio sangue o Mestre comprou-me"
          },
          {
            "chords": "  E                             A... ",
            "lyrics": "A fim de fazer-me na glória entrar."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "          D               A ",
            "lyrics": "Não têm valor a prata_e_o ouro"
          },
          {
            "chords": "                 D",
            "lyrics": "Para dar a salvação;"
          },
          {
            "chords": "                          G  ",
            "lyrics": "Não há, na terra,_algum tesouro"
          },
          {
            "chords": "       D      A      D",
            "lyrics": "Que resgate_e dê perdão."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "D                  A ",
            "lyrics": "O ouro não dá remissão do pecado,"
          },
          {
            "chords": "            D",
            "lyrics": "A prata não leva ao reino dos céus;"
          },
          {
            "chords": "                     G         D",
            "lyrics": "Somente o sangue, na cruz derramado,"
          },
          {
            "chords": "  E                               A... ",
            "lyrics": "Livrou-me da morte, por graça de Deus."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "  D                 A ",
            "lyrics": "O sangue precioso e imaculado"
          },
          {
            "chords": "            D",
            "lyrics": "De Cristo Jesus conduziu-me a Deus;"
          },
          {
            "chords": "                    G       D",
            "lyrics": "É nele que sempre estou abrigado,"
          },
          {
            "chords": "    E                        A... ",
            "lyrics": "Por ele conservo a vida dos céus."
          }
        ]
      },

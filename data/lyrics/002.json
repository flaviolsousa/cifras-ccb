    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "   D           A        G    A7    D",
            "lyrics": "De Deus tu és eleita, igreja de Jesus,"
          },
          {
            "chords": "  F#m        G        Em     D     A",
            "lyrics": "O teu divino Mestre à glória te conduz;"
          },
          {
            "chords": "   D             G   D      Bm    B7   Em",
            "lyrics": "Prepara-te, pois E - le mui breve voltará;"
          },
          {
            "chords": "   D              G        A   A7     D",
            "lyrics": "A glória que te espera já preparada está."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "    D           A        G      A7     D",
            "lyrics": "Com celestiais adornos espera o teu Senhor,"
          },
          {
            "chords": "   F#m        G      Em   D     A",
            "lyrics": "Vestida de justiça e do divino amor;"
          },
          {
            "chords": "  D           G  D       Bm   B7   Em",
            "lyrics": "Dileta és de Cristo, que vida te doou;"
          },
          {
            "chords": "  D            G          A      A7   D",
            "lyrics": "A fim de resgatar-te, Seu sangue derramou."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "   D          A        G    A7    D",
            "lyrics": "É grande tua glória, igreja de Jesus;"
          },
          {
            "chords": "   F#m        G      Em       D    A",
            "lyrics": "Consagra-te a Ele, andando em Sua luz;"
          },
          {
            "chords": "   D             G  D      Bm    B7   Em",
            "lyrics": "Em breve, no Seu reino, gloriosa entrarás,"
          },
          {
            "chords": "    D         G         A      A7    D",
            "lyrics": "E a face do Esposo, no céu, contemplarás."
          }
        ]
      }
    ]
  }

      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "          G                          D                G",
            "lyrics": "É grande bem, precioso bem, viver na fé, ter paz com Deus,"
          },
          {
            "chords": "                                   D             G",

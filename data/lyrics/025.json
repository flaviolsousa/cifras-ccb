        "code": "2",
        "verses": [
          {
            "chords": "            G                             D            G",
            "lyrics": "Que grande bem, precioso bem! Eu sirvo_a Deus, o Formador,"
          },
          {
            "chords": "                                D           G",
            "lyrics": "Que para Si me separou; remido sou do Salvador;"
          },
          {
            "chords": "                               D             G",
            "lyrics": "Purificou meu coração, me concedeu o Seu perdão;"
          },
          {
            "chords": "",

          {
            "chords": "    A                 E",
            "lyrics": "Os Teus caminhos me ensinas;"
          },
          {
            "chords": "   B7                      E",
            "lyrics": "Eu sei, Senhor, que Tu me amas"
          },
          {
            "chords": "    F#m  B7         E",
            "lyrics": "Com Teu amor celestial."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "B       F#7          B ",
            "lyrics": "Oh! Que voz tão preciosa"
          },
          {
            "chords": "  F#7            B  B7",
            "lyrics": "É Tua voz, ó Criador,"
          },
          {
            "chords": "                   E",
            "lyrics": "Meiga, santa e poderosa,"
          },
          {
            "chords": "    F#m  B7          E",
            "lyrics": "Que me  alegra, ó Senhor."

{
  "version": "v2",
  "title": "Só Jesus concede paz e salvação",
  "tone": "C",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "C",
            "lyrics": "Quem, disposto, segue ao Senhor Jesus,"
          },
          {
            "chords": "F             C     D          G ",
            "lyrics": "Com virtude_e força leva_a sua cruz;"
          },
          {
            "chords": "G7          C    Am     F   E ",
            "lyrics": "Tem poder divino em seu coração,"
          },
          {
            "chords": "C           F    D  G7        C",
            "lyrics": "Não será venci - do pela tentação."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "G ",
            "lyrics": "Só Jesus concede paz e salvação,"
          },
          {
            "chords": "                     D           G7 ",
            "lyrics": "E, na glória_eterna, grande galardão;"
          },
          {
            "chords": "C",
            "lyrics": "Quem também deseja ir com Deus morar,"
          },
          {
            "chords": "F            C       G7       C",
            "lyrics": "Sente que, a Cristo, deve aceitar."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "C",
            "lyrics": "Todos os casados, vinde ao Senhor,"
          },
          {
            "chords": "F           C     D            G ",
            "lyrics": "Vinde sem demora, Ele_é_o Salvador;"
          },
          {
            "chords": "G7        C       Am    F    E ",
            "lyrics": "E dizei a Cristo, com satisfação:"
          },
          {
            "chords": "C            F  D   G7        C",
            "lyrics": "“Nós Te consagramos nosso coração.”"
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "C",
            "lyrics": "Com a nova vida no Senhor Jesus"
          },
          {
            "chords": "F             C      D          G ",
            "lyrics": "Mostrareis ao mundo que_Ele é a Luz;"
          },
          {
            "chords": "G7           C    Am    F     E ",
            "lyrics": "Seja_a vossa obra um louvor a Deus,"
          },
          {
            "chords": "C         F  D  G7             C",
            "lyrics": "E a vida_eterna vós tereis nos céus."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
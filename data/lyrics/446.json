{
  "version": "v2",
  "title": "Vós, ó jovens do Senhor",
  "tone": "G",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "       G                         C          G",
            "lyrics": "Vós, ó jovens do Senhor, dai-Lhe honra e louvor,"
          },
          {
            "chords": "                                    D    D7",
            "lyrics": "Esforçai-vos por servir ao Rei dos reis;"
          },
          {
            "chords": "     G                        C           G",
            "lyrics": "Revesti-vos, pois, de fé e do Seu imenso_amor;"
          },
          {
            "chords": "       C         D             G",
            "lyrics": "O que d´Ele precisardes vós tereis."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "       G                                   D",
            "lyrics": "Vós, ó jovens do Senhor, oh! amai-O com fervor,"
          },
          {
            "chords": "                           G",
            "lyrics": "O que Ele vos mandar, procurai executar;"
          },
          {
            "chords": "    A7                D   ",
            "lyrics": "Morareis com Deus em Seu lar;"
          },
          {
            "chords": "    G                                   D",
            "lyrics": "Diligentes sede vós, Ele não vos deixa sós,"
          },
          {
            "chords": "                         G",
            "lyrics": "Mas convosco estará e as forças vos dará,"
          },
          {
            "chords": "       C      D      G",
            "lyrics": "Pois a graça n´Ele_está."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "       G                      C          G",
            "lyrics": "Sois remidos, ó irmãos, sois amados de Jesus"
          },
          {
            "chords": "                                D    D7",
            "lyrics": "E sois fortes para suplantar o mal;"
          },
          {
            "chords": "     G                    C               G",
            "lyrics": "Pelo bom Consolador vós tereis virtude_e luz;"
          },
          {
            "chords": "         C          D        G",
            "lyrics": "Sim, crescei na estatura divinal."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "     G                  C             G",
            "lyrics": "Se vier o tentador com ofertas vos tentar,"
          },
          {
            "chords": "                          D    D7",
            "lyrics": "Incitando-vos a males cometer,"
          },
          {
            "chords": "     G                     C         G",
            "lyrics": "Recorrei ao Salvador, Ele vos virá livrar;"
          },
          {
            "chords": "       C             D             G",
            "lyrics": "Vós, ó jovens, não deveis jamais ceder."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
{
  "version": "v2",
  "title": "A fiel proteção",
  "tone": {
    "recommended": "G"
  },
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "    G                   C",
            "lyrics": "A fiel proteção do Teu manto de amor"
          },
          {
            "chords": "    G         A7          D",
            "lyrics": "Segurança me traz, bom Jesus;"
          },
          {
            "chords": "    G                  C",
            "lyrics": "Abrigado estou, tenho força e valor;"
          },
          {
            "chords": "       G       D",
            "lyrics": "Trilharei Tua senda de luz."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "       D                    G",
            "lyrics": "O teu manto de amor sinto estar sobre mim; "
          },
          {
            "chords": "      A7                  D    D7",
            "lyrics": "Minha vida Te entrego, Senhor;"
          },
          {
            "chords": "     G                C",
            "lyrics": "Viverei pela fé, protegido assim,"
          },
          {
            "chords": "       G        D        G",
            "lyrics": "Na presença de Deus, Criador."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "        G                   C",
            "lyrics": "Meu destino de luz vejo ao longe, Senhor,"
          },
          {
            "chords": "     G       A7        D",
            "lyrics": "E jamais olharei para trás;"
          },
          {
            "chords": "       G                  C",
            "lyrics": "Minha crença em Ti guardarei, Redentor;"
          },
          {
            "chords": "      G         D       G",
            "lyrics": "Teu poder, virtuoso me faz."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "       G                     C",
            "lyrics": "Teu intento, Senhor, vem em mim revelar;"
          },
          {
            "chords": "     G        A7         D",
            "lyrics": "Preciosos são os planos Teus;"
          },
          {
            "chords": "     G                    C",
            "lyrics": "De valor em valor quero aqui terminar"
          },
          {
            "chords": "     G         D         G",
            "lyrics": "A carreira proposta por Deus."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
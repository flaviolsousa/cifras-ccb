{
  "version": "v2",
  "title": "Na plenitude estarei",
  "tone": "D",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": " D     Em   A7          D    Bm7 ",
            "lyrics": "Desejo ser,    ó meu Senhor,"
          },
          {
            "chords": "       E  E7        A    A7       ",
            "lyrics": "Fiel a Ti    até o fim;"
          },
          {
            "chords": "             G  A7        D    Bm7 ",
            "lyrics": "Meus passos gui-a em Teu amor;"
          },
          {
            "chords": "           Em   A7    D    Bm7 ",
            "lyrics": "Senhor, és tudo para mim."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "        Em   A7      D    Bm7 ",
            "lyrics": "Na plenitude    estarei,"
          },
          {
            "chords": "         Em     A7       D    Bm7  ",
            "lyrics": "Os céus abertos    eu verei,"
          },
          {
            "chords": "        Em      A7       D    Bm7 ",
            "lyrics": "Eternas bênçãos    herdarei,"
          },
          {
            "chords": "           Em     A7        D",
            "lyrics": "Servindo a Ti, Senhor, meu Rei."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "D     Em   A7         D    Bm7 ",
            "lyrics": "Veneração,    fé e temor"
          },
          {
            "chords": "        E  E7          A    A7       ",
            "lyrics": "Estejam em    meu coração,"
          },
          {
            "chords": "        G A7       D    Bm7 ",
            "lyrics": "E, do pecado, ó Senhor,"
          },
          {
            "chords": "          Em   A7   D    Bm7 ",
            "lyrics": "Livrar-me pode Tua mão."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "D         Em    A7        D    Bm7 ",
            "lyrics": "As trevas vindo    sobre mim,"
          },
          {
            "chords": "         E   E7              A    A7 ",
            "lyrics": "Não temerei,    pois o esplendor"
          },
          {
            "chords": "        G   A7              D    Bm7 ",
            "lyrics": "Da Tua luz,    que não tem fim,"
          },
          {
            "chords": "        Em     A7      D    Bm7 ",
            "lyrics": "Me guia ao Teu lar, Senhor."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }
}
        "verses": [
          {
            "chords": "     E",
            "lyrics": "Os talentos Jesus nos tem dado,"
          },
          {
            "chords": "       B7                 E",
            "lyrics": "Dons celestes de grande valor;"
          },
          {
            "chords": "",
            "lyrics": "Nós devemos, com todo_o cuidado,"
          },
          {
            "chords": "      B7                   E    E7",
            "lyrics": "Granjeá-los servindo_ao Senhor."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "E7      A                 E",
            "lyrics": "  Granjeai, granjeai os talentos;"
          },
          {
            "chords": "          B7                   E    E7",
            "lyrics": "  Sobre_o muito_o Senhor vos porá;"
          },
          {
            "chords": "      A                  E",
            "lyrics": "  Vigiai e estai sempre_atentos,"
          },
          {
            "chords": "          B7            E",
            "lyrics": "  Eis que Ele à porta está."

        "verses": [
          {
            "chords": "      E",
            "lyrics": "Se em tudo nós formos prudentes,"
          },
          {
            "chords": "     B7                 E",
            "lyrics": "Se fiéis o Senhor nos achar,"
          },
          {
            "chords": "",
            "lyrics": "Herdaremos Seus bens permanentes"
          },
          {
            "chords": "     B7                  E    E7",
            "lyrics": "No repouso da glória sem par."
          }
        ]

      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "B                F#      B",
            "lyrics": "Ó bom Jesus, Teu santo falar"
          },
          {
            "chords": "E        B    C#7     F# ",
            "lyrics": "Sempre comove meu coração;"
          },
          {
            "chords": "B              F#         B",
            "lyrics": "Faz-me um vaso para Te_honrar;"
          },
          {
            "chords": "          E      F#     B",
            "lyrics": "Dá-me, Senhor, a Tua unção."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "B       E               B",
            "lyrics": "Ó bom Jesus, meu caro Jesus,"
          },
          {
            "chords": "          C#7          F# ",
            "lyrics": "Faz deste vaso, ó Redentor,"
          },
          {
            "chords": "B       E              B",
            "lyrics": "Vaso de honra, graça e luz,"
          },
          {
            "chords": "        E       F#       B    B...",
            "lyrics": "Para louvar-Te, ó meu Senhor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "B              F#     B",
            "lyrics": "Neste deserto, caro Jesus,"
          },
          {
            "chords": "E       B      C#7      F# ",
            "lyrics": "Sou peregrino, por Teu amor;"
          },
          {
            "chords": "B              F#      B",
            "lyrics": "Vem dirigir-me com Tua luz,"
          },
          {
            "chords": "        E      F#       B",
            "lyrics": "Sê, ó Jesus, o meu Condutor."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "B             F#      B",
            "lyrics": "Pela Palavra, vem revelar"
          },
          {
            "chords": "E      B    C#7       F# ",
            "lyrics": "Tua vontade, o Teu querer,"
          },
          {
            "chords": "B              F#       B",
            "lyrics": "Pois eu desejo no céu entrar"
          },
          {
            "chords": "         E       F#     B",
            "lyrics": "Para, ao fim, Contigo viver."

{
  "version": "v2",
  "title": "Ajuntemos um tesouro eterno",
  "tone": "B",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "B                                 F#",
            "lyrics": "Ajuntemos, ó meninos, nosso bom tesouro lá nos céus de_amor,"
          },
          {
            "chords": "B                                        E     F#  B",
            "lyrics": "Onde_habita nosso Mestre e o Pai eterno, nosso Criador."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "B",
            "lyrics": "Ajuntemos um tesouro_eterno"
          },
          {
            "chords": "F#                       B",
            "lyrics": "Onde_habita nosso Deus superno."
          },
          {
            "chords": "E    F#     B               F#   B",
            "lyrics": "Nos céus os bens jamais se acabarão."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "B                                      F#",
            "lyrics": "Ricos das eternas bênçãos e dos dons celestes que nos vêm de Deus,"
          },
          {
            "chords": "B                                          E      F#        B",
            "lyrics": "Caminhemos nas veredas da justiça_eterna, que nos leva_aos céus."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "B                                 F#",
            "lyrics": "Ao sairmos deste mundo, tudo deixaremos, tudo findará;"
          },
          {
            "chords": "B                                   E      F#  B",
            "lyrics": "O tesouro ajuntado na eterna glória sempre durará."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      }
    ]
  }

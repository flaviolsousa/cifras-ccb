{
  "version": "v2",
  "title": "Vinde às fileiras",
  "tone": "B",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "B          E  B               F#7 ",
            "lyrics": "Vinde às fileiras do bom Salvador,"
          },
          {
            "chords": "             B      E     C#   F# ",
            "lyrics": "Que vos cingirá com armas de valor,"
          },
          {
            "chords": "B         F#7                B",
            "lyrics": "Para combaterdes em nenhum temor"
          },
          {
            "chords": "E      F#7  B     E     F#7    B",
            "lyrics": "Contra o  pecado, com todo_o vigor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "B               E  B              F#7 ",
            "lyrics": "Cristo, sobre_a morte, foi o Vencedor,"
          },
          {
            "chords": "            B       E   C#    F# ",
            "lyrics": "Sendo, do sepulcro, o Suplantador;"
          },
          {
            "chords": "B           F#7                    B",
            "lyrics": "Sem nenhuma culpa, sobre_a cruz morreu,"
          },
          {
            "chords": "E      F#7    B    E    F#7    B",
            "lyrics": "Mas, vitori - oso, retornou ao céu."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "B             E  B              F#7 ",
            "lyrics": "Cristo, nosso Mestre, Guia celestial,"
          },
          {
            "chords": "             B     E    C#     F# ",
            "lyrics": "É Quem nos defende do poder do mal;"
          },
          {
            "chords": "B             F#7            B",
            "lyrics": "Sempre resplandece com a Sua luz,"
          },
          {
            "chords": "E     F#7   B     E      F#7    B",
            "lyrics": "Na gloriosa senda que ao céu conduz."
          }
        ]
      }
    ]
  }

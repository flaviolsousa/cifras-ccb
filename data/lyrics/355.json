        "verses": [
          {
            "chords": "E              B7        E  B7                E",
            "lyrics": "A Ti, Deus, eu consagrei-me para sempre Te servir;"
          },
          {
            "chords": "                 A    E       B7                  E",
            "lyrics": "De Teus dons vem revestir-me, e de_amor também cingir."
          },
          {
            "chords": "              A      E  E7  A      B7           E ",
            "lyrics": "Vem manter-me sempre firme, bem disposto_a Te ouvir."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "B7                  E",
            "lyrics": "Ao bom Pai, o Deus eterno,"
          },
          {
            "chords": "B7                E",
            "lyrics": "Consagrei meu coração;"
          },
          {
            "chords": "             E7    A ",
            "lyrics": "Com sublime_amor superno"
          },
          {
            "chords": "   B7               E",
            "lyrics": "Me prometeu o galardão."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "E           B7    E  B7              E",
            "lyrics": "Nesta graça preciosa sou ditoso em viver;"
          },
          {
            "chords": "            A    E   B7                E",
            "lyrics": "Que herança gloriosa do Senhor vou receber!"
          },
          {
            "chords": "          A   E  E7  A     B7              E",
            "lyrics": "Esta fé maravilhosa  quero sempre_em mim reter."
          }
        ]
      },
      {

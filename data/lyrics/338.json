{
  "version": "v2",
  "title": "O Senhor é sempiterno",
  "tone": "B",
  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "B               F#7                  B",
            "lyrics": "O Senhor é sempiterno, Ele nunca mudará;"
          },
          {
            "chords": "                  E            F#7         B",
            "lyrics": "Todas as fiéis promessas a Seu tempo cumprirá;"
          },
          {
            "chords": "F#7                 B     C#7            F#  F#7 ",
            "lyrics": "Por Seu sangue_imaculado, trouxe regeneração;"
          },
          {
            "chords": "B                E           F#7            B",
            "lyrics": "Operou, em nossa mente, celestial transformação."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "B                 F#7                     B",
            "lyrics": "Somos reis e sacerdotes feitos para_o Criador,"
          },
          {
            "chords": "             E           F#7     B",
            "lyrics": "E a Ele serviremos em justiça e amor,"
          },
          {
            "chords": "F#7                B     C#7                F#  F#7 ",
            "lyrics": "Para que entrar possamos na presença do bom Deus"
          },
          {
            "chords": "B             E            F#7            B",
            "lyrics": "E viver eternamente com os santos lá nos céus."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "B                F#7                      B",
            "lyrics": "Não percamos a herança que nos veio por Jesus;"
          },
          {
            "chords": "                 E        F#7          B",
            "lyrics": "Breve vamos possuí-la no eterno lar de luz."
          },
          {
            "chords": "F#7              B     C#7            F#  F#7 ",
            "lyrics": "Prossigamos no caminho da cidade do Senhor,"
          },
          {
            "chords": "B                E           F#7        B",
            "lyrics": "Onde juntos reinaremos com o nosso Redentor."
          }
        ]
      }
    ]
  }
}
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "     A",
            "lyrics": "Deus enviou do céu, Jesus, o Salvador,"
          },
          {
            "chords": "  D             A        E ",
            "lyrics": "A fim de, neste mundo, remir o pecador;"
          },
          {
            "chords": "  A",
            "lyrics": "A Cristo vou ouvir e pela fé andar,"
          },
          {
            "chords": "    D             A         E             A",
            "lyrics": "E_a glória,_eternamente, no céu vou desfrutar."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "          E                  A",
            "lyrics": "Avante_eu vou, com Cristo_eu vou,"
          },
          {
            "chords": "                                     E ",
            "lyrics": "Para_entrar no_eterno reino de_esplendor;"
          },
          {
            "chords": "      A                    D           A",
            "lyrics": "Sigo, pela graça_e fé, a Jesus, o Salvador;"
          },
          {
            "chords": "                 E                 A",
            "lyrics": "Nada quero deste mundo,_avante_eu vou."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "   A",
            "lyrics": "No dia do Senhor, os anjos tocarão"
          },
          {
            "chords": "  D             A        E ",
            "lyrics": "A celestial trombeta que todos ouvirão;"
          },
          {
            "chords": "  A",
            "lyrics": "Retornará Jesus, o nosso Redentor,"
          },
          {
            "chords": "   D                A          E             A",
            "lyrics": "Em glória, sobre_as nuvens, cercado de_esplendor."
          }
        ]
      },
      {
        "type": "ref",
        "ref": "score.stanzas[1]"
      },
      {
        "type": "lyric",
        "code": "3",
        "verses": [
          {
            "chords": "  A   ",
            "lyrics": "Jesus, o Salvador, ao céu me levará;"
          },
          {
            "chords": "  D           A         E ",
            "lyrics": "Receberei a_herança que preparada_está;"
          },
          {
            "chords": "  A",
            "lyrics": "A Deus, o Pai, verei, com Cristo estarei"
          },
          {
            "chords": "  D          A         E             A",
            "lyrics": "E todos os remidos, no céu, contemplarei."
          }
        ]
      },

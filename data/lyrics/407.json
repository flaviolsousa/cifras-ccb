  "score": {
    "stanzas": [
      {
        "type": "lyric",
        "code": "1",
        "verses": [
          {
            "chords": "    G",
            "lyrics": "Oh! Quanto amo Tua lei eterna_e divinal!"
          },
          {
            "chords": "  C                    A7          D",
            "lyrics": "E nela eu medito dia_e noite, ó Senhor;"
          },
          {
            "chords": "    G",
            "lyrics": "Por Teus conselhos ando na justiça celestial,"
          },
          {
            "chords": "      C     D   G",
            "lyrics": "Nas veredas de amor."
          }
        ]
      },
      {
        "type": "chorus",
        "verses": [
          {
            "chords": "G                        D",
            "lyrics": "És bendito_eternamente, glorioso Criador!"
          },
          {
            "chords": "G               C  Am  E   Am    D    G",
            "lyrics": "Minha alma, reverente, Te adora, ó Senhor."
          }
        ]
      },
      {
        "type": "lyric",
        "code": "2",
        "verses": [
          {
            "chords": "     G",
            "lyrics": "Teus santos testemunhos me alegram, ó Senhor,"
          },
          {
            "chords": "   C                          A7          D",
            "lyrics": "As Tuas sãs palavras são mais doces que o mel,"
          },
          {
            "chords": "     G",
            "lyrics": "Pois falam das riquezas do Teu reino de_esplendor,"
          },
          {
            "chords": "     C     D    G",
            "lyrics": "Preparadas ao fiel."
          }
        ]
      },
      {
        "type": "ref",
